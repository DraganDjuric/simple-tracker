{
  "templates": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "sourceType": "module",
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "src",
      "README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "node_modules/jsdoc-template"
  }
}
