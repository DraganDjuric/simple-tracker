{
  "name": "simple-tracker",
  "description": "Simple time tracker app built on top of React and Redux.",
  "author": "Tanveer Karim <contact@tkarimdesign.com>",
  "version": "1.3.2",
  "license": "MIT",
  "private": true,
  "homepage": "https://www.simpletracker.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanmancan/simple-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/tanmancan/simple-tracker/issues"
  },
  "keywords": [
    "react",
    "timer",
    "timer-tracking"
  ],
  "dependencies": {
    "materialize-css": "^1.0.0-beta",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "release:patch": "npm version patch -m 'Release v%s'",
    "release:minor": "npm version minor -m 'Release v%s'",
    "release:major": "npm version major -m 'Release v%s'",
    "preversion": "npm run test",
    "version": "npm run build",
    "postversion": "git push && git push --tag",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "doc": "jsdoc -c .jsdoc.json && cp react-timer.png docs/react-timer.png"
  },
  "devDependencies": {
    "jsdoc": "^3.5.5",
    "jsdoc-template": "git+https://github.com/braintree/jsdoc-template.git#3.2.0",
    "redux-devtools": "^3.4.1"
  }
}
